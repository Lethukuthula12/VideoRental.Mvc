@model VideoRent.Models.Movie
@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>
<br /><br /><br />
<h2>@Model.Name</h2>
<ul>
   
   
       <li>Genre:@Model.Genre.Name</li>
       <li>Released Date: @Model.ReleasedDate.ToLongDateString()</li>
       <li>Date Added: @Model.DateAdded.ToLongDateString()</li>
       <li>Number in Stock: @Model.NumberOfStock</li>
   

</ul>

